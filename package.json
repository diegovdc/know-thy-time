{
  "name": "time-budget",
  "version": "0.1.0",
  "description": "ClojureScript project setup with Shadow-CLJS",
  "scripts": {
    "release": "run-s release:*",
    "release:app": "shadow-cljs release app",
    "watch:app": "shadow-cljs watch app",
    "release:library": "shadow-cljs release library",
    "watch:library": "shadow-cljs watch library",
    "release:browser": "shadow-cljs release browser && npm run sass",
    "watch:browser": "shadow-cljs watch browser",
    "start:browser": "serve -s build/browser",
    "sass:watch": "sass src/sass/styles.scss build/browser/styles.css --watch",
    "sass": "sass src/sass/styles.scss build/browser/styles.css"
  },
  "keywords": [],
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "serve": "^11.1.0",
    "shadow-cljs": "^2.8.39",
    "sass": "^1.30.0"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.5.3",
    "date-fns": "^2.16.1",
    "react": "16.13.0",
    "react-bootstrap": "^1.4.0",
    "react-bootstrap-icons": "^1.2.2",
    "react-color": "^2.19.3",
    "react-dom": "16.13.0"
  }
}
